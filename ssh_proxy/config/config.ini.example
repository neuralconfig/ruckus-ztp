# RUCKUS ZTP SSH Proxy Configuration
# Copy this file to /etc/ruckus-ztp-proxy/config.ini and update values

[server]
# WebSocket server URL (required)
url = wss://your-backend-server.com/ws/ssh-proxy

# Authentication token (required)
# Generate a secure token and share it with the backend server
token = your-secure-token-here

[proxy]
# Unique proxy ID (optional - will be auto-generated if not provided)
# id = proxy-001

# Reconnection interval in seconds (default: 30)
reconnect_interval = 30

# SSH command timeout in seconds (default: 60)
command_timeout = 60

# Maximum concurrent commands (default: 1)
max_concurrent_commands = 1

[logging]
# Log level: DEBUG, INFO, WARNING, ERROR (default: INFO)
level = INFO

# Log file path (optional - logs to stdout if not specified)
file = /var/log/ruckus-ztp-proxy/proxy.log